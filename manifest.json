{
  "manifest_version": 2,
  "name": "HoneyMoney importer",
  "description": "Export transactions from web-banking and import them into https://app.hmbee.ru",
  "version": "1.0",
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "images/button.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab",
    "tabs",
    "https://hmbee.ru/"
  ],
  "offline_enabled": false,
  "minimum_chrome_version": "45.0.2454.85"
}